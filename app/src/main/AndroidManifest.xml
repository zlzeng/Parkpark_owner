<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zz.parkpark_owner" >

    <uses-feature android:name="android.hardware.Camera2" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateParkingActivity"
            android:label="@string/title_activity_create_parking" >
        </activity>
        <activity
            android:name=".SetAddressActivity"
            android:label="@string/title_activity_set_address" >
        </activity>
        <activity
            android:name=".SetPriceActivity"
            android:label="@string/title_activity_set_price" >
        </activity>
        <activity
            android:name=".SetTimeActivity"
            android:label="@string/title_activity_set_time" >
        </activity>
        <activity
            android:name=".SetTimeDetailActivity"
            android:label="@string/title_activity_set_time_detail" >
        </activity>
        <activity
            android:name=".VerifyActivity"
            android:label="@string/title_activity_verify" >
        </activity>
        <activity
            android:name=".SetUserAddrActivity"
            android:label="@string/title_activity_set_user_addr" >
        </activity>
        <activity
            android:name=".SetUserPhoneActivity"
            android:label="@string/title_activity_set_user_phone" >
        </activity>
    </application>

</manifest>
